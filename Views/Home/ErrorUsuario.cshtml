<!DOCTYPE html>
<html lang="es">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=9; IE=8; IE=7; IE=edge" />

    <meta http-equiv="Expires" content="0" />
    <meta http-equiv="Pragma" content="no-cache" />

    <link href="~/favicon.ico" rel="shortcut icon" />
    <title>Error Usuario</title>
    <link rel="stylesheet" type="text/css" href="~/css/estilos.css">
    <link rel="stylesheet" type="text/css" href="~/css/simulador.css">
    <link href="~/css/loader.css" rel="stylesheet" />
    <style>
        /* Hide page by default */
        html {
            display: none;
        }
    </style>
</head>

<body onload="handleBackButton()">
    @{
        ViewData["Title"] = "Logout";

        Layout = "~/Views/Shared/_errorLayout.cshtml";
    }
    <!-- Contenido -->
    <div class="contSecc">
        <br>
        <br>
        <div class="titV">  @ViewData["tituloPag"] </div>

    </div>

    <div id="modalLogoutUsr" class="modal">
        <div class="cuadro cuadroM1">
            <input type="hidden" id="urlR" value="@ViewData["url"]" />
            <a href="#" id="btnCerrarLogout" class="simplemodal-close btnCerrar"><img src="/SimuladorAbonos/img/icoCerrar.svg"></a>
            <div class="titModal">Aviso</div>
            <br />
            <div>
                <br>
                <span style="font-size:18px; text-align:center!important"> @ViewData["msjSesion"] </span>
                <br><br>

                <span style="font-size:18px; text-align:center!important"> @ViewData["msjSesion2"] </span>
                <br><br>
            </div>


        </div>
    </div>


    <div class="clear"></div><br>
    <script>
        if (self == top) {
            // Everything checks out, show the page.
            document.documentElement.style.display = 'block';
        } else {
            top.location = @ViewData["url"];
        }
    </script>
</body>

</html>

<script type="text/javascript" src="~/js/jquery-1.12.4.min.js"></script>
<script type="text/javascript" src="~/js/jquery.simplemodal.js"></script>
<script type="text/javascript" src="~/js/errorUsuario.js"></script>

