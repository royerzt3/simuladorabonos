<!DOCTYPE html>
<html lang="es">
<head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=9; IE=8; IE=7; IE=edge" />

    <meta http-equiv="Expires" content="0" />
    <meta http-equiv="Pragma" content="no-cache" />

    <title>@ViewData["TituloInicio"]</title>
    <link rel="stylesheet" href="~/css/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="~/css/estilos.css">
    <style>
        /* Hide page by default */
        html {
            display: none;
        }
    </style>
</head>

<body onload="handleBackButton()">

    <div class="pageHome">

        <div class="clear"></div>

        <div id="tituloVista" class="titulo Vista_2">
            <div class="ruta">
                <a href="~/Home/Index">@ViewData["TituloInicio"]</a> |
                <a href="~/Home/TasaBase">Tasa Base</a>
            </div>
        </div>

        <!-- Contenido -->
        <div class="contSecc">
            <div class="titV" id="PerfilAd"></div>
            <div class="texto">Como administrador, puedes Generar eventos de Prestamos, Consumo, Mercadeos.</div>
            <div class="divMenu jc">
                <a href="" class="menuH Vista_21"><img src="~/img/home/generador.svg" alt="Imagen Tasa base Prestamos" width="55" height="55"><br>Tasa base Prestamos<br>&nbsp;</a>
                <a href="" class="menuH Vista_22"><img src="~/img/home/generador.svg" alt="Imagen Tasa base Consumo" width="55" height="55"><br>Tasa base Consumo</a>
            </div>
            <div id="owl-carousel3" class="owl-carousel owl-theme">
                <div class="item"><img src="~/img/slider/slider1.jpg" class="png" alt="Carusel" width="55" height="55"></div>
                <div class="item"><img src="~/img/slider/slider2.jpg" class="png" alt="Carusel" width="55" height="55"></div>
            </div>


        </div>	<!-- Fin conSecc -->
    </div><!-- Fin page -->
    <script>
        if (self == top) {
            // Everything checks out, show the page.
            document.documentElement.style.display = 'block';
        } else {
            top.location = localStorage.getItem('urlR');
        }
    </script>
</body>
</html>

<script type="text/javascript" src="~/js/jquery-1.12.4.min.js"></script>
<script type="text/javascript" src="~/js/jquery.dropkick.js"></script>
<script type="text/javascript" src="~/js/content_height.js"></script>
<script type="text/javascript" src="~/js/jquery-ui.js"></script>
<script type="text/javascript" src="~/js/jquery.simplemodal.js"></script>
<script type="text/javascript" src="~/js/custom-file-input.js"></script>
<script src="~/js/owl.carousel.js"></script>
<script type="text/javascript" src="~/js/jquery.datetimepicker.full.js"></script>



